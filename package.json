{
  "name": "nestjs-redis-cache",
  "version": "1.0.0",
  "description": "A NestJS-compatible Redis caching package with explicit caching decorators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "publish:npm": "npm publish --access public",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com/",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "publish:release": "npm run release && git push --follow-tags origin $(git rev-parse --abbrev-ref HEAD) && npm publish --access public"
  },
  "keywords": [
    "nestjs",
    "redis",
    "cache",
    "caching",
    "decorator",
    "typescript"
  ],
  "author": "hmake98",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "ioredis": "^5.3.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
